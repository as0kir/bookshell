<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="
        http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

      <view-state id="main">
              <on-render>
                  <evaluate expression="testBean.test()"/>
                  <evaluate expression="bookDaoImpl.books"/>
                  <evaluate expression="bookDaoImpl.getBooks(&quot;author=vvv&quot;)"/>
                  <evaluate expression="bookDaoImpl.getBooks(&quot;printYear=2017&quot;)"/>
                  <evaluate expression="bookDaoImpl.getBooks(&quot;readAlready=1&quot;)"/>
              </on-render>
    </view-state>
</flow>